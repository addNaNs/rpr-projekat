<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<GridPane fx:id="pane" xmlns="http://javafx.com/javafx/10.0.2-internal" xmlns:fx="http://javafx.com/fxml/1">
    <rowConstraints>
        <RowConstraints minHeight="10.0" prefHeight="30.0" />
        <RowConstraints minHeight="10.0" prefHeight="30.0" />
    </rowConstraints>
    <columnConstraints>
        <ColumnConstraints maxWidth="-Infinity" minWidth="10.0" prefWidth="160.0" />
        <ColumnConstraints maxWidth="-Infinity" minWidth="10.0" prefWidth="150.0" />
      <ColumnConstraints />
    </columnConstraints>
    <children>
        <Label text="%categories">
            <padding>
                <Insets left="5.0" />
            </padding></Label>
        <HBox alignment="CENTER_RIGHT" prefHeight="100.0" prefWidth="200.0" GridPane.columnSpan="3" GridPane.rowIndex="1">
            <children>
                <Button mnemonicParsing="false" onAction="#clickCancel" text="%cancel" />
                <Button mnemonicParsing="false" onAction="#clickOk" text="%ok" />
            </children></HBox>
        <TextField fx:id="fieldCategories" GridPane.columnIndex="1" />
        <Separator prefWidth="200.0" GridPane.columnSpan="2" GridPane.rowIndex="1" GridPane.valignment="TOP" />
    </children>
</GridPane>
